generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Status {
  PLANNING
  IN_PROGRESS
  ON_HOLD
  COMPLETED
}

enum InitType {
  ACADAFFAIRS
  COMMS
  DI
  IT
  SL
}

model init {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  type        InitType
  leaders     leader[] @relation("inittoleader")
  createdAt   DateTime @default(now())
  lastUpdated DateTime @default(now())
  status      Status   @default(PLANNING)
}


model leader {
  id               Int                @id @default(autoincrement())
  name             String
  contact          String
  inits            init[]       @relation("inittoleader")
}

    